// â”œâ”€â”€â”€â”€â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”¤
// â”‚ CapsL   â”‚ ğ‘¨ É‘ â”‚ ğ‘– Êƒ â”‚ ğ‘¼ Ã° â”‚ ğ‘² Ê â”‚ Â· Ê¤ â”‚ ğ‘ â€‘ â”‚ ğ‘¡ Ê§ â”‚ Â« ËŒ â”‚ Â» É« â”‚ : Ë â”‚ " â˜’ â”‚   enter  â”‚
// â”‚         â”‚ ğ‘© â€œ â”‚ ğ‘• â€ â”‚ ğ‘› â†“ â”‚ ğ‘“ Â° â”‚ ğ‘œ É› â”‚ ğ‘£ â€ â”‚ ğ‘˜ Éœ â”‚ ğ‘’ â‰¤ â”‚ ğ‘¤ â‰¥ â”‚ ; Â§ â”‚ ' Ã— â”‚          â”‚
// â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤
// â”‚    shift   â”‚ ğ‘  Ê’ â”‚ ğ‘º Ã¦ â”‚ ğ‘½ Â¢ â”‚ ğ‘¿ ÊŒ â”‚ â¸° â—‹ â”‚ ğ‘™ Å‹ â”‚ ğ‘µ Âµ â”‚ â€¹ â˜º â”‚ â€º â˜ â”‚ ? â˜‘ â”‚    shift    â”‚
// â”‚            â”‚ ğ‘Ÿ â€˜ â”‚ ğ‘» â€™ â”‚ ğ‘— Â© â”‚ ğ‘ âƒ â”‚ ğ‘š Â· â”‚ ğ‘¯ â€“ â”‚ ğ‘¥ â€” â”‚ , < â”‚ . > â”‚ / Ã· â”‚             â”‚
// â”œâ”€â”€â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”¬â”€â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”€â”€â”¼â”€â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”€â”¤
// â”‚       â”‚      â”‚      â”‚ space            zero-width space â”‚      â”‚       â”‚      â”‚      â”‚
// â”‚ ctrl  â”‚ win  â”‚  alt â”‚ space                    M-space  â”‚ Ralt â”‚       â”‚      â”‚ ctrl â”‚
// â””â”€â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”€â”˜
// ```
// --------------------
// Unused Unicode range U+FDD0-U+FDEF
// These code points will be translated by ~/.XCompose according to following table:
// UFDD0 = É”Éª  // q U0254 U026A
// UFDD1 = aÊŠ  // Q U0061 U028A
// UFDD2 = ÉªÉ™  // W U026A U0259
// UFDD3 = eÉª  // E U0065 U026A
// UFDD4 = É‘Ër // R U0251 U02D0 U0072
// UFDD5 = É™ÊŠ  // O U0259 U028A
// UFDD6 = É”Ër // P U0254 U0072
// UFDD7 = É™r  // D U0259 U0072
// UFDD8 = aÉª  // F U0061 U026A
// UFDD9 = dÊ’  // J U0064 U0292
// UFDDA = ÉœËr // x U025C U02D0 U0072
// UFDDB = É›É™r // X U025B U0259 U0072 
// UFDDC = tÊƒ  // c U0074 U0283
// UFDDD = ÉªÉ™r // C U026A U0259 U0072
// UFDDE = ju  // V U006A U0075
// --------------------
// if you use a rare/never used keysym in the keymap definition, then a global Xmodmap to make THAT keysym output the various unicode characters you need, this'll work.
// In the keymap:
//     key <AB04>  { [ v, V, XF86LaunchA, XF86LaunchB ] };
// In a global Xmodmap: (perhaps loaded from /etc/profile.d ?)
//     keysym XF86LaunchA = U0056 U0331
//     keysym XF86LaunchB = U0076 U0331
//There are quite a few unused/special-use keysyms, I chose the LaunchA/B as an example.
//  XF86Launch[0,1,2..D,E,F]  - 16 total
// full list: /usr/include/X11/XF86keysym.h
// --------------------
// it is possible to use the Unicode code directly in xkb keyboard layout definition files in /usr/share/X11/xkb/symbols/ or to use with keysym using this format U#### or 0x100####.
//Example:
//	key <AD04> { [        r, R,           U200F,        U200E           ] };
//Or to define new keysym:
//	xmodmap -e "keysym RLM = U200F"
//For permanent keysym, add to ~/.xmodmap file:
//	keysym RLM = U200F
//References:
//	Custom xkb layout in which one key creates two Unicode code points
//	FOSS Localization/Annex B: Technical Aspects
//	Creating and Compiling Custom Keymaps for Linux
// --------------------


default partial alphanumeric_keys modifier_keys
xkb_symbols "phonem" {

//    include "us(basic)"
    name[Group1]= "English (Phonemic)";

    key <TLDE>	{[ grave,	asciitilde,	U00B4,		U2248	]}; // ` ~ Â´ (acute accent)  â‰ˆ (almost equal)
    key <AE01>	{[ 1,		exclam,		U00B9,		U2081	]}; // 1 ! Â¹ (superscript 1) â‚ (subscript 1)
    key <AE02>	{[ 2,		at,		U00B2,		U2082	]}; // 2 @ Â² (superscript 2) â‚‚ (subscript 2)
    key <AE03>	{[ 3,		numbersign,	U00b3,		U2083	]}; // 3 # Â³ (superscript 3) â‚ƒ (subscript 3)
    key <AE04>	{[ 4,		dollar,		U2074,		U2084	]}; // 4 $ â´ (superscript 4) â‚„ (subscript 4)
    key <AE05>	{[ 5,		percent,	U2075,		U2085	]}; // 5 % âµ (superscript 5) â‚… (subscript 5)
    key <AE06>	{[ 6,		asciicircum,	U2076,		U2086	]}; // 6 ^ â¶ (superscript 6) â‚† (subscript 6)
    key <AE07>	{[ 7,		ampersand,	U2077,		U2087	]}; // 7 & â· (superscript 7) â‚‡ (subscript 7)
    key <AE08>	{[ 8,		asterisk,	U2078,		U2088	]}; // 8 * â¸ (superscript 8) â‚ˆ (subscript 8)
    key <AE09>	{[ 9,		parenleft,	U2079,		U2089	]}; // 9 ( â¹ (superscript 9) â‚‰ (subscript 9)
    key <AE10>	{[ 0,		parenright,	U2070,		U2080	]}; // 0 ( â° (superscript 0) â‚€ (subscript 0)
    key <AE11>	{[ minus,	underscore,	U2192,		U2190	]}; // - _ â†’ (right arrow)   â† (left arrow)
    key <AE12>	{[ equal,	plus,		U2260,		U00B1	]}; // = + â‰  (notequal to)   Â± (plus minus)

    key <AD01> {[ UFDD0,	UFDD1,		U2026,		U231C	]}; //q: É”Éª     aÊŠ     â€¦ (ellipsis) âŒœ (top-L corner)
    key <AD02> {[ w,		UFDD2,		U22EF,		U231D	]}; //w: w      ÉªÉ™     â‹¯ (hor elli) âŒ (top-R corner)
    key <AD03> {[ U025b,	UFDD3,		U0301,		U231E	]}; //e: É›      eÉª     eÌ (acute ac) âŒ (bot-L corner)
    key <AD04> {[ r,		UFDD4,		U00AE,		U231F	]}; //r: r      É‘r     Â· (registrd) âŒŸ (bot-R corner)
    key <AD05> {[ t,		U03B8,		U2122,		U03B8	]}; //t: t      Î¸      â„¢ (trademak) Î¸ (theta)
    key <AD06> {[ U0251,	U0254,		U221A,		U0259	]}; //y: É‘      É”      âˆš (sq root)  É™ (schwa)
    key <AD07> {[ U028C,	U028A,		U2191,		U028A	]}; //u: ÊŒ      ÊŠ      â†‘ (up arrow) ÊŠ (upsilon)
    key <AD08> {[ U026A,	i,		U026A,		U02C8	]}; //i: Éª      i      Éª (small I)  Ëˆ (high stress)
    key <AD09> {[ U0252,	UFDD5,		U0252,		U0254	]}; //o: É’      É™ÊŠ     É’ (tur alph) É” (open o)
    key <AD10> {[ p,		UFDD6,		U2117,		U03C0	]}; //p: p      É”r     â„— (sound co) Ï€ (pie)
    key <AD11> {[ bracketleft,	braceleft,	U27E8,		U27EE	]}; //   [      {      âŸ¨ (angle br) âŸ® (math flat paren)
    key <AD12> {[ bracketright,	braceright,	U27E9,		U27EF	]}; //   ]      }      âŸ© (angle br) âŸ¯ (math flat paren)
    key <BKSL> {[ backslash,	bar,		U2022,		U25E6	]}; //   \      |      â€¢ (bullet)   â—¦ (white bullet)

    key <AC01>	{[ U0259,	U00E6,		U201C,		U0251	]}; //a: É™      Ã¦      â€œ (L d quot)  É‘ (alpha)
    key <AC02>	{[ s,		U0283,		U201D,		U0283	]}; //s: s      Êƒ      â€ (R d quot)  Êƒ (esh)
